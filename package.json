{
  "name": "hackerai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"npx convex dev\"",
    "dev:next": "next dev --turbopack",
    "dev:convex": "npx convex dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app lib types __mocks__ --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "pnpm install && npx tsx scripts/setup.ts",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' jest --maxWorkers=2",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=4096' jest --watch",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096' jest --coverage --maxWorkers=2",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=4096' jest --ci --coverage --maxWorkers=2",
    "prepare": "husky",
    "e2b:build:dev": "npx tsx e2b/build.dev.ts",
    "e2b:build:prod": "npx tsx e2b/build.prod.ts",
    "s3:validate": "npx tsx scripts/validate-s3-security.ts"
