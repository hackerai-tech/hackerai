    "@workos-inc/node": "^7.73.0",
    "ai": "^5.0.93",
    "ai-elements": "^1.6.3",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.29.2",
    "date-fns": "^4.1.0",
    "e2b": "^2.7.0",
    "gpt-tokenizer": "^3.4.0",
    "isbinaryfile": "^5.0.7",
    "jszip": "^3.10.1",
    "langchain": "^1.0.4",
    "lucide-react": "^0.554.0",
    "mammoth": "^1.11.0",
    "marked": "^17.0.0",
    "motion": "^12.23.24",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "openai": "^6.9.0",
    "pdfjs-serverless": "^1.1.0",
    "posthog-js": "^1.294.0",
    "posthog-node": "^5.11.2",
    "react": "^19.2.0",
